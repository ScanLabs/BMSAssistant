<script>
const graphInterval = %SHOW_GRAPH_INTERVAL%;
let xLabel=0;
var chartCPWNet = new Highcharts.Chart({
  chart:{ 
	renderTo : 'chart-HouseNet',	
	zoomType: 'xy'},
  title: { text: 'Battery Pack Power flow<br>SOC %packSOC% &#37;' },
  series: [{
	name: 'Battery Pack Charge',
	threshold: 0,
        negativeColor: 'red',
        color: 'green',
	pointStart: Date.UTC(%SHOW_GRAPH_POINT_START%),
        pointInterval: graphInterval  
    }],
  plotOptions: {
    line: { 
         animation: false,
         dataLabels: {
	  formatter: function() {
            if (this.y > 20 || this.y < -20) {
           	if ( xLabel++>=10) {xLabel=0;if (this.y>999 || this.y<-999) return (this.y/1000); else return (this.y);}
            }
	  },
	  enabled: true
	}
    },
    series: {
            marker: {
                enabled: false,
                states: {
                    hover: {
                        enabled: false
                    }
                }
            }
        }
  },
  xAxis: {type:'datetime',
    dateTimeLabelFormats:{second:'%H:%M'}
  },
  yAxis:{title:{text:'Power [W]'}},
  labels:{format:'{value}'},
  credits:{enabled:true},
  accessibility:{enabled:false}
});
setInterval(function ( ) {
  window.location.reload();
}, 60*1000) ;
function fillData(){
  let SolarBTChargerDayHistory=[%SHOW_GRAPH_BTSOL_CHARGER%];
  for (let i = 0; i < SolarBTChargerDayHistory.length; i++){
    if (SolarBTChargerDayHistory[i]==undefined) SolarBTChargerDayHistory[i]=0;
    if (i) SolarBTChargerDayHistory[i] = SolarBTChargerDayHistory[i] + SolarBTChargerDayHistory[i-1];
  }
  chartCPWNet.series[0].setData(SolarBTChargerDayHistory, true);
}
function openTab(boxName,elmnt,color) {
  var i, tabcontent, tablinks;
  tabcontent = document.getElementsByClassName("tabcontent");
  for (i = 0; i < tabcontent.length; i++) {
    tabcontent[i].style.display = "none";
  }
  tablinks = document.getElementsByClassName("tablink");
  for (i = 0; i < tablinks.length; i++) {
    tablinks[i].style.backgroundColor = "";
  }
  document.getElementById(boxName).style.display = "block";
  elmnt.style.backgroundColor = color;
}
document.getElementById("defaultOpen").click();
fillData();
</script>
</html>
