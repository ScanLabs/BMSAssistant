    tablinks[i].style.backgroundColor = "";
  }
  document.getElementById(boxName).style.display = "block";
  elmnt.style.backgroundColor = color;
}
function dashBoardConfig( extcomp ){ //1-extract 0-compress
  let dashBoardCfg1=%TaD1%;   
  let dashBoardCfg2=%TaD2%;

  if ( extcomp ){
    let chartEnabled=(dashBoardCfg1 & 0x04000000)?1:0;
    let chartTimeSpan=(dashBoardCfg1 & 0xF8000000)>>27;
    let autoRolling=(dashBoardCfg1 & 0x02000000)?1:0;
    let autoBrightness=(dashBoardCfg1 & 0x01000000)?1:0;
    let brLevelMax=(dashBoardCfg1 & 0x00FF0000)>>16;
    let brLevelAvg=(dashBoardCfg1 & 0x0000FF00)>>8;
    let brLevelMin=(dashBoardCfg1 & 0x000000FF)>>0;

    let autoRollSec=(dashBoardCfg2 & 0xFF000000)>>24;
    let brLevelMaxS=(dashBoardCfg2 & 0x00FFF000)>>12;
    let brLevelAvgS=(dashBoardCfg2 & 0x00000FFF)>>0;

    document.getElementById("ChEn").checked=chartEnabled;
    document.getElementById("ChT").value=chartTimeSpan;
    document.getElementById("AuRo").checked=autoRolling;
    document.getElementById("ROs").value=autoRollSec;
    document.getElementById("AuBE").checked=autoBrightness;
    document.getElementById("bMx").value=brLevelMax;
    document.getElementById("bMe").value=brLevelAvg;
    document.getElementById("bLw").value=brLevelMin;
    document.getElementById("bMxs").value=brLevelMaxS;
    document.getElementById("bMes").value=brLevelAvgS;
 }else
 {
    dashBoardCfg1 = document.getElementById("ChEn").checked?(1<<26):0;  
    dashBoardCfg1+= ((document.getElementById("ChT").value & 0xFF)<<27);
    dashBoardCfg1+= document.getElementById("AuRo").checked?(1<<25):0;
    dashBoardCfg1+= document.getElementById("AuBE").checked?(1<<24):0;
    dashBoardCfg1+= ((document.getElementById("bMx").value & 0xFF)<<16);
    dashBoardCfg1+= ((document.getElementById("bMe").value & 0xFF)<<8);
    dashBoardCfg1+= (document.getElementById("bLw").value & 0xFF);
    
    dashBoardCfg2 = (document.getElementById("ROs").value & 0xFF) <<24;
    dashBoardCfg2+= (document.getElementById("bMxs").value & 0xFFF) << 12;
    dashBoardCfg2+= (document.getElementById("bMes").value & 0xFFF) << 0;

    var urltocall="/get_oh?TaD1="+dashBoardCfg1+"&TaD2="+dashBoardCfg2;
    xmlhttp=new XMLHttpRequest();
    xmlhttp.open("GET", urltocall, false);
    xmlhttp.send();
 }
}
function DoChecks(parIn){
  document.getElementById("DyPg").value&=0xFF;
  if ( !document.getElementById("DyPg").value)
    document.getElementById("DyPg").value=1;
  if ( document.getElementById("TaEn").checked && parIn==0){
    document.getElementById("oHEn").checked=false;
  }
  if ( document.getElementById("oHEn").checked && parIn==1){
    document.getElementById("TaEn").checked=false;
  }
  if ( document.getElementById("oHEn").checked==true || document.getElementById("TaEn").checked==false ||
   document.getElementById("DyPg").value!=1){
    document.getElementById("AuRo").disabled=document.getElementById("ROs").disabled=true;
    document.getElementById("ChEn").disabled=document.getElementById("ChT").disabled=true;
    document.getElementById("AuBE").disabled=true;
    document.getElementById("bMx").disabled=document.getElementById("bMe").disabled=document.getElementById("bLw").disabled = true;
    document.getElementById("bMxs").disabled =document.getElementById("bMes").disabled=true;
  }
  if ( document.getElementById("TaEn").checked==true &&
   document.getElementById("DyPg").value==1){
    document.getElementById("ChEn").disabled=document.getElementById("ChT").disabled=false;
    document.getElementById("AuRo").disabled=document.getElementById("ROs").disabled=false;
    document.getElementById("AuBE").disabled=false;
    document.getElementById("bMx").disabled=document.getElementById("bMe").disabled=document.getElementById("bLw").disabled=false;
    document.getElementById("bMxs").disabled =document.getElementById("bMes").disabled=false;
  }

  if ( document.getElementById("AuBE").checked==true){
    document.getElementById("bMx").disabled=document.getElementById("bMe").disabled=document.getElementById("bLw").disabled=true;
    document.getElementById("bMxs").disabled =document.getElementById("bMes").disabled=true;
  }
  if( document.getElementById("AuRo").checked==false){
    document.getElementById("ROs").disabled=true;
  }
  if ( document.getElementById("ChEn").checked==false)
    document.getElementById("ChT").disabled=true;

  if (!%mqttPublishEnable% && parIn<2){
    alert("Display requires MQTT to be configured and active");
  }
}
