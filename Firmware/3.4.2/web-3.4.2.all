function populateBatts(){
  var maxRows=Math.trunc(battsNow.length / 3)+1;
  var maxColums=battsNow.length;
  var max=battsNow.reduce((iMax,x,i,arr)=> x > arr[iMax] ? i :iMax,0);
  let min=battsNow.reduce((iMax,x,i,arr)=> x < arr[iMax] ? i :iMax,0);
  let battArrayString="<table class=\"center\">";
  let battID=0;
  if (maxColums > 2) 
    maxColums=3;
  for (row=0; row < maxRows; row++){
    battArrayString +="<tr>";
    for (cln=0; cln < maxColums ; cln++){
      if (battID < battsNow.length){
        battsNow[battID]=(battsNow[battID]/1000).toFixed(3);
        battArrayString += "<td style=\"width:16%%\"><button class=\"buttonn button1\">&nbsp"+ ("0"+ (battID+1)).slice(-2) + "&nbsp</button></td>";
        if (battID==max)
          battArrayString += "<td style=\"width:16%%;text-align:left;color:red;\">"+ battsNow[battID] + "<font5 style=\"color:red\">V</font5></td>";
        else if (battID==min)
          battArrayString += "<td style=\"width:16%%;text-align:left;color:white;\">"+ battsNow[battID] + "<font5 style=\"color:#fffffe\">V</font5></td>";
        else
          battArrayString += "<td style=\"width:16%%;text-align:left;color:#41cd52;\">"+ battsNow[battID] + "<font5>V</font5></td>";
        battID++;
      }
    }
    battArrayString +="</tr>"; 
  }
  battArrayString +="</table>"; 
  document.getElementById("allBatts").innerHTML=battArrayString;  
}
function populateAlms(){
  let theString="<th1>Alarms:<a href=\"/bmsalarms\"style=\"color:";
