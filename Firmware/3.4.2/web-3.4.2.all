   URL.revokeObjectURL(link.href);
};

const downloadPanelFile = () => {
   const link = document.createElement("a");
   let content = "";
   for (let i = 0; i < scanlabs_jsonl.length; i++) {
      content += String.fromCharCode(scanlabs_jsonl[i]);
   }
   if ( mqttMOS ){        
   }
   content = content.replaceAll("*!openHaspPanelID*!", openHaspPanelID );
 
   const file = new Blob([content], { type: 'text/plain' });
   link.href = URL.createObjectURL(file);
   link.download = "scanlabs.jsonl";
   link.click();
   URL.revokeObjectURL(link.href);
};
const downloadExtraFile = ( dataSource, fileName ) => {
