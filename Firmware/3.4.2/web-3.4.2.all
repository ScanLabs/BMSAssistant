<!DOCTYPE HTML><html>
<head>
  <title>SmartBMS WiFi</title>    
  <style>
body {
 min-width: 310px;
 max-width: 1000px;
 height: 400px;
 margin: 0 auto;
 font-family: "Lato", sans-serif;
}
h2 {
 font-family: Arial;
 font-size: 2.5rem;
 text-align: center;
}
.tablink {
  background-color: #555;
  color: white;
  float: left;
  border: none;
  border-radius: 10px;
  outline: none;
  cursor: pointer;
  padding: 8px 12px;
  font-size: 13px;
  width: 20%% ;
}
.tablink:hover {
  background-color: #777;
}
.tabcontent {
  color: black;
  display: none;
  padding: 20px;
  text-align: center;
}
.btn-group button {
  background-color: #04AA6D; 
  border: 1px solid green; 
  border-radius: 12px;
  margin: 4px 2px;
  color: white; 
  padding: 10px 24px; 
  cursor: pointer; 
  float: left;  
}
.btn-group:after {
  content: "";
  clear: both;
  display: table;
}
.btn-group button:not(:last-child) {
  border-right: none; 
}
.btn-group button:hover {
  background-color: #3e8e41;
}
</style> 
  <meta name="viewport" content="width=device-width, initial-scale=1">
</head>

<script>
  function submitMessage() {
    let stringAdj = document.getElementById("BMSF").value.toUpperCase().replace(/\s+/g, '');
    document.theForm.inputCommand.value = stringAdj; 

    console.log(document.getElementById("BMSF").value);
    alert("Message Delivered: " + document.getElementById("BMSF").value );

    setTimeout(function(){ document.location.reload(false); }, 500);   
  }
function openTab(boxName,elmnt,color) {
  var i, tabcontent, tablinks;
  tabcontent = document.getElementsByClassName("tabcontent");
  for (i = 0; i < tabcontent.length; i++) {
    tabcontent[i].style.display = "none";
  }
  tablinks = document.getElementsByClassName("tablink");
  for (i = 0; i < tablinks.length; i++) {
    tablinks[i].style.backgroundColor = "";
  }
  document.getElementById(boxName).style.display = "block";
  elmnt.style.backgroundColor = color;
}
function populate(){
  document.getElementById("BMSF").setAttribute('value',document.getElementById("BMSC").value);
}
</script>
<body>
  <button class="tablink" onclick="window.location.href='/';">Status</button> 
  <button class="tablink" onclick="window.location.href='/batt';">Battery</button>
  <button class="tablink" onclick="window.location.href='/graph';">Monitor</button>
  <button class="tablink" onclick="openTab('Settings', this, 'red');" id="defaultOpen">Settings</button>
  <button class="tablink" onclick="window.location.href='/fwupgrade';">Upgrade</button>
  <div id="Settings" class="tabcontent"></div>
  <div style='margin:15px;'><br><center><b>DEBUG TOOLS</center></b><br>
  <center>ID %platformMAC%</center><br>
  <hr><br>
  <form name="theForm" action="/get" target="hidden-form" onsubmit="submitMessage();">
    <span title="Send a command to BMS: NOTE HEX format HH">Command to BMS: <input type="text" name="inputCommand" id="BMSF" size="30"></span>
  <select name="bmsCommands" id="BMSC" onchange="populate()">
  <option value="">Custom</option>
  <option value="1d20600f000009a5a">BMS Reset</option>
  <option value="A580D908010000000000000007">Discharge FET ON-1</option>
  <option value="d20600a60001bb8a">Discharge FET ON-2</option>
  <option value="A580D908000000000000000006">Discharge FET OFF-1</option>
  <option value="d20600a600007a4a">Discharge FET OFF-2</option>
  <option value="D20600A500014B8A">Charge FET ON</option>
  <option value="D20600A500008A4A">Charge FET OFF</option>
</select>
    <input type="submit" value="Submit">
  </form><br>
  <div class="btn-group">
    <button onclick="window.location.href='config';">BACK to CONFIG</button>
    <button onclick="document.location.reload(false);">RELOAD</button>
  </div>
  <textarea id="dongleLog" name="BMSLogTX" rows="50" cols="132">
  %BMS_REPEATER_BUFFER_LOG%
  </textarea>
  <iframe style="display:none" name="hidden-form"></iframe>
  <script>
    populate();
    document.getElementById("defaultOpen").click();
  </script>
</body></html><!--FFILE-->
